{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es2021": true,
    "jest": true
  },
  "plugins": ["prettier"],
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "google"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "script"
  },
  "ignorePatterns":["node_modules/"],
  "rules": {
    "prettier/prettier": "error"
  }
}